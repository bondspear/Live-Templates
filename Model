namespace VendorName\ModuleName\Model;
/**
*if we need rest we use where we get constants
*use VendorName\ModuleName\Api\Data\ModelClassNameInterface;implements ModelClassNameInterface 
*/

use Magento\Framework\Model\AbstractModel;

class YouModelName extends AbstractModel implements \Magento\Framework\DataObject\IdentityInterface
{

    /**
     * @inheritdoc
     */
    protected function _construct()
    {
        $this->_init('VendorName\ModuleName\Model\ResourceModel\YouModelName');
    }

    public function getIdentities()
    {
        return [$this->getId()];
    }

    public function getDefaultValues()
    {
        $values = [];

        return $values;
    }
    
     /**
     * @return int
     */
    public function getId()
    {
        return $this->getData(ModelClassNameInterface::ID);
    }

    /**
     * @param int $authorId
     * @return $this
     */
    public function setId($id)
    {
        $this->setData(ModelClassNameInterface::ID, $id);
        return $this;
    }
}
