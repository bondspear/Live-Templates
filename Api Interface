/**
 * Interface ModelClassNameInterface
 * @package VendorName\ModuleName\Api\Data
 * @api
 */
interface ModelClassNameInterface
{
    /**#@+
     * Constants
     * @var string
     */
    const ID         = 'id';
    const SOME       = 'some';
    const CREATED_AT = 'created_at';
    const UPDATED_AT = 'updated_at';
